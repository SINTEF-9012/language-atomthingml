{
  "name": "language-thingml",
  "version": "0.0.0",
  "main": "./lib/main",
  "description": "ThingML language support in Atom",
  "engines": {
    "atom": "*"
  },
  "activationCommands": {
    "atom-workspace": [
      "language-thingml:check",
      "language-thingml:compile-arduino",
      "language-thingml:compile-nodejs",
      "language-thingml:compile-java",
      "language-thingml:compile-posix"
    ]
  },
  "dependencies": {
    "atom-message-panel": "1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SINTEF-9012/language-atomthingml"
  },
  "bugs": {
    "url": "https://github.com/SINTEF-9012/language-atomthingml/issues"
